load "Samples.m";

harder:=["IsMaximal"];


function initg(g,nm)
  G:=New(LMFDBGrp);
  G`Label:=nm;
  G`MagmaGrp:=g;
  AssignBasicAttributes(G);
  for j in harder do dummy:=Get(G,j); end for;
  return(G);
end function;

for k->v in samples do
  samples[k] := [initg(z, GroupName(z)) : z in v];
end for;

G := New(LMFDBGrp);
G`Label := "whateva";
G`MagmaGrp := Alt(9);
AssignBasicAttributes(G);

